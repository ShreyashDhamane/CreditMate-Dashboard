{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@app/*": ["./src/@next/*"],
      "public/*": ["./public/*"],
      "@components/*": ["./src/components/*"],
      "@components/atoms": ["./src/@next/components/atoms/*"],
      "@components/molecules": ["./src/@next/components/molecules/*"],
      "@components/organisms": ["./src/@next/components/organisms/*"],
      "@components/templates": ["./src/@next/components/templates/*"],
      "@hooks": ["./src/@next/hooks/index.ts"],
      "@core/*": ["./src/core/*"],
      "@utils/*": ["./src/core/utils.ts"],
      "@images/*": ["./src/assets/styles/images/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/page.jsx", "src/app/layout.jsx", "src/components/Layout/Layout.jsx", "src/components/Sidebar/Sidebar.jsx"],
  "exclude": ["node_modules"]
}
